{
  "name": "voyager-inc/quote-replace",
  "description": "A package to replace quote in string",
  "type": "library",
  "keywords": [
    "quote-replace",
    "voyager-inc"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Phu Tran Dinh",
      "email": "phutd@voyager-hcm.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4|^5|^6|^7|^8|^9"
  },
  "autoload": {
    "psr-4": {
      "VoyagerInc\\QuoteReplace\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VoyagerInc\\QuoteReplace\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "VoyagerInc\\QuoteReplace\\ServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": false
}
